<?xml version="1.0" encoding="utf-8" ?>
<sqlpal>
    <!-- 数据库 -->
    <database>mysql</database>
    <!-- 驱动程序名 -->
    <driver>com.mysql.jdbc.Driver</driver>
    <!-- 连接数据库的URL -->
    <url>jdbc:mysql://localhost:3306/test?useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=false</url>
    <!-- 数据库用户名 -->
    <username>root</username>
    <!-- 数据库密码 -->
    <password>123456</password>
    <!-- 最大批处理数量 -->
    <maxBatch>10000</maxBatch>
    <!-- 映射列表 -->
    <list>
        <!-- 用户表 -->
        <mapping>org.sqlpal.sample.model.User</mapping>
        <!-- 新闻表 -->
        <mapping>org.sqlpal.sample.model.News</mapping>
    </list>
</sqlpal>